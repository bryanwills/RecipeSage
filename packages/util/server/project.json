{
  "name": "util-server",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/util/server/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "dependsOn": ["util-shared:build"],
      "options": {
        "main": "packages/util/server/src/index.ts",
        "outputPath": "dist/libs/util/server",
        "tsConfig": "packages/util/server/tsconfig.lib.json",
        "declaration": true,
        "assets": [],
        "platform": "node"
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/util/server",
        "commands": [
          "tsc --project tsconfig.lib.json --noEmit",
          "tsc --project tsconfig.spec.json --noEmit"
        ],
        "parallel": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "maxWarnings": 0
      }
    }
  }
}
